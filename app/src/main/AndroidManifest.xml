<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sharepreferenceapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SharePreferenceApplication">
        <activity
            android:name=".sharepref.ViewDataActivity"
            android:exported="true" >

        </activity>
        <activity
            android:name=".sharepref.ForgotPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".sharepref.DisplayDataActivity"
            android:exported="false" />
        <activity
            android:name=".sharepref.RegistrationActivity"
            android:exported="false" />
        <activity
            android:name=".sharepref.SingInActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>



        </activity>
        <activity
            android:name=".simpleLogin.DisplayActivity"
            android:exported="false" />
        <activity
            android:name=".simpleLogin.LoginActivity"
            android:exported="true">

        </activity>
        <activity
            android:name=".simpleLogin.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >



        </activity>
    </application>

</manifest>